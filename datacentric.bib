	Month = jun,
	Pages = {154-165},
	Pdf = {papers/hpcview-ics01.pdf},
	Ps = {papers/hpcview-ics01.ps.gz},
	Title = {Tools for Application-Oriented Performance Tuning},
	Year = 2001}

